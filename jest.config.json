{
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  },
  "setupFilesAfterEnv": ["./jest-setup.js"],
  "coveragePathIgnorePatterns": [
    "prototype"
  ],
  "moduleNameMapper": {
    "^@(\/.*)$": [
      "<rootDir>/src/$1"
    ]
  },
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.vue$": "vue-jest"
  },
  "globals": {
    "__DEBUG__": true,
    "__BASE_URL__": "/"
  }
}
